{
  "name": "sdk-graph-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build @senzing/sdk-graph-components && ng serve",
    "build": "ng build @senzing/sdk-graph-components && node build-package.js",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "package": "npm run build && cd ./dist/@senzing/sdk-graph-components/ && npm pack && cd ../../../",
    "publish": "cd ./dist/@senzing/sdk-graph-components/ && npm publish --access restricted",
    "test": "ng test",
    "test:headless": "ng test --no-watch --watch=false --progress=false --no-progress --browsers=ChromeHeadlessCI",
    "watch": "rm -fR dist/@senzing/sdk-graph-components && concurrently --kill-others \"wait-on file:dist/@senzing/sdk-graph-components/public_api.d.ts && ng serve\" \"ng build @senzing/sdk-graph-components --watch\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@senzing/rest-api-client-ng": "^1.0.2",
    "core-js": "^2.5.4",
    "d3": "^5.9.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/d3": "^5.7.1",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "wait-on": "^3.2.0"
  }
}
